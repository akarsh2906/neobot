<?xml version="1.0" ?>
<sdf version="1.8">

    <model name='neobot'>

    <!-- ********************* PLUGINS ********************* -->

        <plugin filename="gz-sim-imu-system"
                name="gz::sim::systems::Imu">
        </plugin>

        <plugin filename="gz-sim-sensors-system"
                name="gz::sim::systems::Sensors">
            <render_engine>ogre2</render_engine>
        </plugin>

        <plugin filename="gz-sim-diff-drive-system"
            name="gz::sim::systems::DiffDrive">
            <left_joint>left_wheel_joint</left_joint>
            <right_joint>right_wheel_joint</right_joint>
            <!-- wheel_separation: Distance b/w outer surfaces of the wheel -->
            <wheel_separation>0.352</wheel_separation>  
            <wheel_radius>0.05</wheel_radius>
            <odom_publish_frequency>30</odom_publish_frequency>
            <min_linear_velocity>-0.314</min_linear_velocity>
            <max_linear_velocity>0.314</max_linear_velocity>
            <min_angular_velocity>-0.4</min_angular_velocity>
            <max_angular_velocity>0.4</max_angular_velocity>
            <topic>/cmd_vel</topic>
            <odom_topic>odom_encoders</odom_topic>
            <tf_topic>tf</tf_topic>
            <frame_id>odom</frame_id>
            <child_frame_id>base_footprint</child_frame_id>
        </plugin>

        <plugin filename="libgz-sim-odometry-publisher-system"
            name="gz::sim::systems::OdometryPublisher">
            <odom_frame>odom</odom_frame>
            <robot_base_frame>base_footprint</robot_base_frame>
            <odom_topic>odom_world</odom_topic>
            <tf_topic>tf_truth</tf_topic>
            <dimensions>2</dimensions>
            <frame_id>odom</frame_id>
            <child_frame_id>base_footprint</child_frame_id>
            <odom_publish_frequency>10</odom_publish_frequency>
        </plugin>

        <plugin filename="gz-sim-joint-state-publisher-system"
            name="gz::sim::systems::JointStatePublisher">
            <joint_name>left_wheel_joint</joint_name>
            <joint_name>right_wheel_joint</joint_name>
            <topic>/joint_states</topic>
        </plugin>



    <!-- ********************* LINKS ********************* -->

        <link name="base_footprint">
        </link>

        <link name='base_link'>
            <pose relative_to='base_footprint_joint'>0 0 0 0 0 0</pose>
            <inertial>
                <pose>0.019736266638338499 -0.00103592208239938 0.024943774276299499 0 0 0</pose>
                <mass>2.8860000000000001</mass>
                <inertia>
                <ixx>0.0230886</ixx>
                <ixy>-0.00059632880</ixy>
                <ixz>0.00089059654</ixz>
                <iyy>0.040993868</iyy>
                <iyz>-0.00001729413</iyz>
                <izz>0.056762850</izz>
                </inertia>
            </inertial>
            <collision name='base_link_collision_frame'>
                <pose>0 0 0.037 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.34 0.26 0.120</size>
                    </box>
                </geometry>
            </collision>
            <collision name='base_link_collision_lidar'>
                <pose>0.18 0 0.026 0 0 0</pose>
                <geometry>
                    <cylinder>
                        <length>0.0526</length>
                        <radius>0.04</radius>
                    </cylinder>
                </geometry>
            </collision>
            <collision name='base_link_collision_camera'>
                <pose>0.184025 0 0.072 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.02805 0.09 0.025</size>
                    </box>
                </geometry>
            </collision>
            <visual name='base_link_visual'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                <mesh>
                    <scale>1 1 1</scale>
                    <uri>../meshes/base_link.STL</uri>
                </mesh>
                </geometry>
                <material>
                <diffuse>0.941176474 0.941176474 0.941176474 1</diffuse>
                <ambient>0.941176474 0.941176474 0.941176474 1</ambient>
                </material>
            </visual>  
        </link>

        <link name='camera_link'>
            <pose relative_to='camera_joint'>0 0 0 0 0 0</pose>

            <!-- <sensor name="camera_link" type="depth_camera">
                <update_rate>30.0</update_rate>
                <gz_frame_id>camera_link</gz_frame_id>
                <topic>camera_link</topic>
                <camera>
                    <camera_info_topic>camera_link/camera_info</camera_info_topic>
                    <horizontal_fov>1.20428</horizontal_fov>
                    <image>
                        <width>640</width>
                        <height>480</height>
                        <format>R8G8B8</format>
                    </image>
                    <clip>
                        <near>0.05</near>
                        <far>4.0</far>
                    </clip>
                </camera>
            </sensor> -->




            <sensor name="camera_link" type="rgbd_camera">
                <update_rate>30</update_rate>
                <topic>camera_link</topic>
                <gz_frame_id>camera_link</gz_frame_id>
                <camera name="camera_link">
                
                <horizontal_fov>1.0472</horizontal_fov>
                <lens>
                    <intrinsics>
                    <!-- fx = fy = width / ( 2 * tan (hfov / 2 ) ) -->
                    <fx>554.25469</fx>
                    <fy>554.25469</fy>
                    <!-- cx = ( width + 1 ) / 2 -->
                    <cx>320.5</cx>
                    <!-- cy = ( height + 1 ) / 2 -->
                    <cy>240.5</cy>
                    <s>0</s>
                    </intrinsics>
                </lens>
                <distortion>
                    <k1>0.0</k1>
                    <k2>0.0</k2>
                    <k3>0.0</k3>
                    <p1>0.0</p1>
                    <p2>0.0</p2>
                    <center>0.5 0.5</center>
                </distortion>
                <image>
                    <width>640</width>
                    <height>480</height>
                    <format>R8G8B8</format>
                </image>
                <clip>
                    <near>0.01</near>
                    <far>300</far>
                </clip>
                <depth_camera>
                    <clip>
                    <near>0.1</near>
                    <far>10</far>
                    </clip>
                </depth_camera>
                <noise>
                    <type>gaussian</type>
                    <mean>0</mean>
                    <stddev>0.007</stddev>
                </noise>
                </camera>
            </sensor>





        </link>

        <link name='imu_link'>
            <pose relative_to='imu_joint'>0 0 0 0 0 0</pose>
            
            <sensor name="imu_sensor" type="imu">
                <gz_frame_id>imu</gz_frame_id>
                <always_on>1</always_on>
                <update_rate>1</update_rate>
                <visualize>true</visualize>
                <topic>imu</topic>
            </sensor>
            
        </link>

        <link name='lidar_link'>
            <pose relative_to='lidar_joint'>0 0 0 0 0 0</pose>
            
            <sensor name='gpu_lidar' type='gpu_lidar'>

                <topic>scan</topic>
                <gz_frame_id>lidar_link</gz_frame_id>
                <update_rate>10</update_rate>
                <ray>
                    <scan>
                        <horizontal>
                            <samples>180</samples>
                            <resolution>1</resolution>
                            <min_angle>-1.57</min_angle>
                            <max_angle>1.57</max_angle>
                        </horizontal>
                        <vertical>
                            <samples>1</samples>
                            <resolution>0.01</resolution>
                            <min_angle>0</min_angle>
                            <max_angle>0</max_angle>
                        </vertical>
                    </scan>
                    <range>
                        <min>0.08</min>
                        <max>8.0</max>
                        <resolution>0.01</resolution>
                    </range>
                </ray>
                <always_on>1</always_on>
                <visualize>true</visualize>
            </sensor>
            
        </link>

        <link name='left_wheel_link'>
            <pose relative_to='left_wheel_joint'>0 0 0 0 0 0</pose>
            <inertial>
                <pose>0 0.015369 0 0 0 0</pose>
                <mass>0.132</mass>
                <inertia>
                <ixx>0.000103824895</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.000183869513</iyy>
                <iyz>0</iyz>
                <izz>0.000103824895</izz>
                </inertia>
            </inertial>
            <collision name='left_wheel_link_collision'>
                <pose>0 0.02 0 1.57079632679 0 0</pose>
                <geometry>
                    <cylinder>
                        <length>0.04</length>
                        <radius>0.05</radius>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                    <ode>
                        <mu>5.00</mu>
                        <mu2>5.00</mu2>
                    </ode>
                    </friction>
                </surface>
            </collision>
            <visual name='left_wheel_link_visual'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                <mesh>
                    <scale>1 1 1</scale>
                    <uri>../meshes/left_wheel_link.STL</uri>
                </mesh>
                </geometry>
                <material>
                <diffuse>0.872550011 0.872550011 0.872550011 1</diffuse>
                <ambient>0.872550011 0.872550011 0.872550011 1</ambient>
                </material>
            </visual>
        </link>

        <link name='right_wheel_link'>
            <pose relative_to='right_wheel_joint'>0 0 0 0 0 0</pose>
            <inertial>
                <pose>0 -0.015369 0 0 0 0</pose>
                <mass>0.132</mass>
                <inertia>
                <ixx>0.000103824895</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.000183869513</iyy>
                <iyz>0</iyz>
                <izz>0.000103824895</izz>
                </inertia>
            </inertial>
            <collision name='right_wheel_link_collision'>
                <pose>0 -0.02 0 1.57079632679 0 0</pose>
                <geometry>
                    <cylinder>
                        <length>0.04</length>
                        <radius>0.05</radius>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                    <ode>
                        <mu>5.00</mu>
                        <mu2>5.00</mu2>
                    </ode>
                    </friction>
                </surface>
            </collision>
            <visual name='right_wheel_link_visual'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                <mesh>
                    <scale>1 1 1</scale>
                    <uri>../meshes/right_wheel_link.STL</uri>
                </mesh>
                </geometry>
                <material>
                <diffuse>0.872550011 0.872550011 0.872550011 1</diffuse>
                <ambient>0.872550011 0.872550011 0.872550011 1</ambient>
                </material>
            </visual>
        </link>

        <link name='front_castor_link'>
            <pose relative_to='front_castor_joint'/>
            <inertial>
                <pose>0 0 0 0 0 0</pose>
                <mass>0.0138</mass>
                <inertia>
                <ixx>0.000000312122139</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.000000312122139</iyy>
                <iyz>0</iyz>
                <izz>0.000000312122139</izz>
                </inertia>
            </inertial>
            <collision name='front_castor_link_collision'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                <sphere>
                    <radius>0.0075</radius>
                </sphere>
                </geometry>
                <!-- <surface>
                    <friction>
                    <ode>
                        <mu>0.00</mu>
                        <mu2>0.00</mu2>
                    </ode>
                    </friction>
                </surface> -->
            </collision>
            <visual name='front_castor_link_visual'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                <mesh>
                    <scale>1 1 1</scale>
                    <uri>../meshes/front_castor_link.STL</uri>
                </mesh>
                </geometry>
                <material>
                <diffuse>0.808824956 0.774512529 0.735300064 1</diffuse>
                <ambient>0.808824956 0.774512529 0.735300064 1</ambient>
                </material>
            </visual>
        </link>

        <link name='rear_castor_link'>
            <pose relative_to='rear_castor_joint'/>
            <inertial>
                <pose>0 0 0 0 0 0</pose>
                <mass>0.0138</mass>
                <inertia>
                <ixx>0.000000312122139</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.000000312122139</iyy>
                <iyz>0</iyz>
                <izz>0.000000312122139</izz>
                </inertia>
            </inertial>
            <collision name='rear_castor_link_collision'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                <sphere>
                    <radius>0.0075</radius>
                </sphere>
                </geometry>
                <!-- <surface>
                    <friction>
                    <ode>
                        <mu>0.00</mu>
                        <mu2>0.00</mu2>
                    </ode>
                    </friction>
                </surface> -->
            </collision>
            <visual name='rear_castor_link_visual'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                <mesh>
                    <scale>1 1 1</scale>
                    <uri>../meshes/rear_castor_link.STL</uri>
                </mesh>
                </geometry>
                <material>
                <diffuse>0.808824956 0.774512529 0.735300064 1</diffuse>
                <ambient>0.808824956 0.774512529 0.735300064 1</ambient>
                </material>
            </visual>
        </link>

        

    <!-- ********************* JOINTS ********************* -->

        <joint name='base_footprint_joint' type='fixed'>
            <pose relative_to='base_footprint'>0 0 0.0345 0 0 0</pose>
                <parent>base_footprint</parent>
                <child>base_link</child>
        </joint>

        <joint name='camera_joint' type='fixed'>
            <pose relative_to='base_link'>0.19905 0 0.072 0 0 0</pose>
                <parent>base_link</parent>
                <child>camera_link</child>
        </joint>

        <joint name='lidar_joint' type='fixed'>
            <pose relative_to='base_link'>0.18 0 0.0376 0 0 0</pose>
                <parent>base_link</parent>
                <child>lidar_link</child>
        </joint>

        <joint name='imu_joint' type='fixed'>
            <pose relative_to='base_link'>0 0 0 0 0 0</pose>
                <parent>base_link</parent>
                <child>imu_link</child>
        </joint>

        <joint name='left_wheel_joint' type='revolute'>
            <pose relative_to='base_link'>0 0.136 0.0155 0 0 0</pose>
                <parent>base_link</parent>
                <child>left_wheel_link</child>
                    <axis>
                        <xyz>0 1 0</xyz>
                        <limit>
                        <velocity>9.11</velocity>
                        <lower>-inf</lower>
                        <upper>inf</upper>
                        </limit>

                    </axis>
        </joint>

        <joint name='right_wheel_joint' type='revolute'>
            <pose relative_to='base_link'>0 -0.136 0.0155 0 0 0</pose>
                <parent>base_link</parent>
                <child>right_wheel_link</child>
                    <axis>
                        <xyz>0 1 0</xyz>
                        <limit>
                            <!-- 9.11 rad/s joint_velocity ~= 87rpm -->
                            <velocity>9.11</velocity>
                            <lower>-inf</lower>
                            <upper>inf</upper>
                        </limit>
                    </axis>
        </joint>

        <joint name='front_castor_joint' type='ball'>
            <pose relative_to='base_link'>0.12 0 -0.0265 0 0 0</pose>
                <parent>base_link</parent>
                <child>front_castor_link</child>
        </joint>
        
        <joint name='rear_castor_joint' type='ball'>
            <pose relative_to='base_link'>-0.12 0 -0.0265 0 0 0</pose>
                <parent>base_link</parent>
                <child>rear_castor_link</child>
        </joint>
        
    </model>

</sdf>